<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="AvatarImageBehavior">
    <attr name="finalYPosition" format="dimension"/>
    <attr name="startXPosition" format="dimension"/>
    <attr name="startToolbarPosition" format="dimension"/>
    <attr name="startHeight" format="dimension"/>
    <attr name="finalHeight" format="dimension"/>
  </declare-styleable>

  <declare-styleable name="DiagonalLayout">
    <!--<attr name="diagonal_angle" format="integer" />
    <attr name="diagonal_handleMargins" format="boolean"/>-->
    <attr name="diagonal_position" format="enum">
      <enum name="left" value="1" />
      <enum name="right" value="2" />
      <enum name="bottom" value="4" />
      <enum name="top" value="8"/>
    </attr>

    <attr name="diagonal_direction" format="enum">
      <enum name="left" value="1" />
      <enum name="right" value="2" />
    </attr>
  </declare-styleable>
  
</resources>